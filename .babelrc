{
    "presets": ["babel-preset-expo"],
    "env": {
      "development": {
        "plugins": ["@babel/transform-react-jsx-source"]
      }
    },
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "components": "./components",
            "hooks": "./hooks",
            "navigation": "./navigation",
            "styles": "./styles",
            "screens": "./screens",
            "stores": "./stores",
            "utils": "./utils",
            "assets": "./assets",
            "constants": "./constants",
            "const": "./const",
            "types": "./types"
          }
        }
      ]
    ]
  }
  