{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["@babel/transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "hooks": "./hooks",
          "navigation": "./navigation",
          "styles": "./styles",
          "screens": "./screens",
          "texts-i18n": "./texts-i18n",
          "utils": "./utils",
          "assets": "./assets",
          "constants": "./constants",
          "const": "./const",
          "types": "./types"
        }
      }
    ]
  ]
}
